{
  "name": "strapi",
  "version": "0.1.0",
  "private": true,
  "description": "A Strapi application",
  "scripts": {
    "build": "strapi build",
    "console": "strapi console",
    "deploy": "strapi deploy",
    "dev": "cross-env ENV_PATH=../.env NODE_OPTIONS=\"--max-old-space-size=4096\" strapi develop",
    "develop": "cross-env ENV_PATH=../.env NODE_OPTIONS=\"--max-old-space-size=4096\" strapi develop",
    "start": "cross-env ENV_PATH=../.env strapi start",
    "strapi": "cross-env ENV_PATH=../.env strapi",
    "upgrade": "pnpm dlx @strapi/upgrade latest",
    "upgrade:dry": "pnpm dlx @strapi/upgrade latest --dry",
    "generate-spec": "cross-env ENV_PATH=../.env strapi openapi generate --output ../openapi/strapi-spec.json"
  },
  "dependencies": {
    "@strapi/plugin-documentation": "^5.23.6",
