{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "level": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "Beginner",
        "Intermediate",
        "Advanced"
      ]
    },
    "description": {
      "type": "blocks"
    },
    "cover_image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "course_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::course-category.course-category",
      "inversedBy": "courses"
    },
    "authors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "courses"
    },
    "course_sections": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::course-section.course-section",
      "mappedBy": "course"
    }
  }
}
